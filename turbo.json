{
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", "build/**"]
		},
		"lint": {
			"dependsOn": ["@sapphire/prettier-config#build", "@sapphire/eslint-config#build"],
			"outputs": []
		},
		"test": {
			"dependsOn": ["@sapphire/time-utilities#build", "@sapphire/fetch#build"],
			"outputs": ["coverage/**"]
		},
		"bump": {
			"dependsOn": [],
			"outputs": ["CHANGELOG.md"]
		},
		"check-update": {
			"dependsOn": [],
			"outputs": []
		},
		"docs": {
			"dependsOn": [],
			"outputs": ["docs/**"]
		}
	}
}
