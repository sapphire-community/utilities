{
	"cleanOutputDir": true,
	"emit": true,
	"entryPointStrategy": "packages",
	"entryPoints": ["."],
	"exclude": [
		"**/node_modules/**",
		"**/dist/**",
		"**/*.test.ts",
		"packages/**/tests/**",
		"**/*.js",
		"**/*.d.ts",
		"**/rollup.config.ts",
		"**/jest.config.ts",
		"**/jest.setup.ts"
	],
	"intentionallyNotExported": [
		"ArgumentTypes",
		"PrettierSchema",
		"InternalAsyncQueueDeferredPromise",
		"Ctor",
		"Awaitable",
		"Nullish",
		"EmbedData",
		"EmbedInformation",
		"EmbedInitialInformation"
	],
	"excludeExternals": false,
	"hideGenerator": true,
	"lightHighlightTheme": "github-light",
	"darkHighlightTheme": "github-dark",
	"markedOptions": {
		"gfm": true,
		"mangle": false,
		"smartypants": true
	},
	"name": "Sapphire Utilities",
	"out": "./docs/",
	"readme": "./README.md",
	"tsconfig": "./tsconfig.dev.json"
}
