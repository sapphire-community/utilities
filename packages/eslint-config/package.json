{
	"name": "@sapphire/eslint-config",
	"version": "4.4.2",
	"description": "Standard ESLint config for the Sapphire Community",
	"author": "@sapphire",
	"license": "MIT",
	"main": "dist/index.js",
	"exports": {
		"import": "./dist/index.js",
		"require": "./dist/index.js"
	},
	"sideEffects": false,
	"homepage": "https://github.com/sapphiredev/utilities/tree/main/packages/eslint-config",
	"scripts": {
		"test": "vitest run",
		"prelint": "yarn test && yarn build",
		"build": "tsup",
		"prepack": "yarn build",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run"
	},
	"dependencies": {
		"@typescript-eslint/eslint-plugin": "^5.59.6",
		"@typescript-eslint/parser": "^5.59.6",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"prettier": "^2.8.8",
		"typescript": "^5.0.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sapphiredev/utilities.git",
		"directory": "packages/eslint-config"
	},
	"files": [
		"dist/**/*.js*",
		"dist/**/*.mjs*",
		"dist/**/*.d*"
	],
	"engines": {
		"node": ">=v14.0.0",
		"npm": ">=7.0.0"
	},
	"keywords": [
		"sapphiredev",
		"bot",
		"typescript",
		"ts",
		"yarn",
		"discord",
		"sapphire",
		"standalone"
	],
	"bugs": {
		"url": "https://github.com/sapphiredev/utilities/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^2.0.0",
		"@vitest/coverage-c8": "^0.31.1",
		"tsup": "^6.7.0",
		"typedoc-json-parser": "^8.1.0",
		"vitest": "^0.31.1"
	}
}
